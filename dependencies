  dependencies_failed_count=$((dependencies_failed_count+1))
  while :; do
    check_command zoxide &
    check $! 'check zoxide' 'zoxide not found'
    if [ $? -ne 0 ]; then
      installer_name=brew
      installer_options=''
      if [[ $installer_name == 'apt' ]]; then
        installer_options='-y'
      fi
      brew install zoxide $installer_options &> /dev/null &
      if [ $? -ne 0 ]; then
        $nsudo brew install zoxide $installer_options &> /dev/null &
      fi
      check $! "brew install zoxide" "brew install zoxide failed"
      if [ $? -eq 0 ]; then
