        break
      fi
    else
      dependencies_failed_count=$((dependencies_failed_count-1))
      break
    fi
    break
  done
}

function dependencies_optional_install() {
  dependencies_failed_count=$((dependencies_failed_count+1))
  while :; do
    check_command meld &
    check $! 'check meld' 'meld not found'
    if [ $? -ne 0 ]; then
      installer_name=$binstaller
      installer_options=''
      if [[ $installer_name == 'apt' ]]; then
        installer_options='-y'
      fi
      $binstaller install meld $installer_options &> /dev/null &
      if [ $? -ne 0 ]; then
        $nsudo $binstaller install meld $installer_options &> /dev/null &
      fi
      check $! "$binstaller install meld" "$binstaller install meld failed"
      if [ $? -eq 0 ]; then
        dependencies_failed_count=$((dependencies_failed_count-1))
        break
      fi
    else
      dependencies_failed_count=$((dependencies_failed_count-1))
      break
    fi
    break
  done
}

