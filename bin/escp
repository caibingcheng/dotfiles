#!/usr/bin/expect -f

set src     [lindex $argv 0]
set dst     [lindex $argv 1]

# try all passwds
set timeout 10
spawn scp $src $dst
for {set i 2} {$i < $argc} {incr i} {
    set passwd [lindex $argv $i]
    expect {
        "yes/no" { send "yes\r"; exp_continue }
        "password:" { send "$passwd\r" }
    }
    expect {
        "Permission denied" {}
        "100%" { break }
    }
}
interact
