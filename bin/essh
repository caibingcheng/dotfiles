#!/usr/bin/expect -f

set dst     [lindex $argv 0]
# try all passwds
set timeout 10
spawn ssh $dst
for {set i 1} {$i < $argc} {incr i} {
    set passwd [lindex $argv $i]
    expect {
        "yes/no" { send "yes\r"; exp_continue }
        "password:" { send "$passwd\r" }
    }
    expect {
        "Permission denied" {}
        "~#" { break }
    }
}
interact
